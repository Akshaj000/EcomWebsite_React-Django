  width: \"100%\",\n            \"background-color\":\"lightgrey\",\n            \"padding\" : \"3px 13px\",\n            \"box-sizing\": \"border-box\",\n            \"border\": \"3px\",\n            \"margin-bottom\":\"20px\",\n            \"border-radius\": \"4px\",\n        }\n\n        const formstyle = {\n            \"border-color\":'black',\n            \"display\":\"none\",\n            \"padding\":\"15px\",\n            \"margin\" : \"10px\",\n        }\n\n        const buttonstyle ={\n            \"margin-left\" : \"13px\",\n            \"margin-top\" : \"5px\"\n        }\n\n        return (\n        <div class=\"card bg-dark\" style={formstyle} id={this.props.id}>\n        <h5>{this.props.head}</h5>\n        <br/>\n        <form onSubmit={this.handleSubmit}>\n            <input\n                type=\"text\"\n                name=\"categoryname\"\n                ref={node => (this.inputNode = node)}\n                style = {textboxstyle}\n                onChange={this.handleChange}\n                defaultValue={this.props.defaultvalue}\n                id = {this.props.id}\n            />\n            <section>\n            <button style={buttonstyle} class=\"btn btn-sm btn-success\" type=\"submit\">{this.props.name}</button>\n            <button  style={buttonstyle} onClick={()=>this.cancelButton(this.props.id)} class=\"btn btn-sm btn-secondary\" type=\"submit\">Cancel</button>\n            </section>\n            \n        </form>\n        </div>\n        )\n    }\n}\n\nexport default CategoryForm","/mnt/1660E61660E5FC7D/AK360/REACT/React_Todolist/todolist/src/components/card.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/React_Todolist/todolist/src/components/category.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/React_Todolist/todolist/src/components/cardform.js",["129"],[],"import React from \"react\";\nimport axios from 'axios';\n\nclass CardForm extends React.Component { \n    state = {\n        name: ''\n    }\n\n    handleChange = (event) => {\n        this.setState({ name: event.target.value });\n    }\n\n\n    render() {\n    \n        return (\n        <div class=\"card bg-dark\" id={this.props.id}>\n        <h5>{this.props.head}</h5>\n        <br/>\n        <form onSubmit={this.handleSubmit}>\n            <input\n                type=\"text\"\n                name=\"taskname\"\n                ref={node => (this.inputNode = node)}\n\n                onChange={this.handleChange}\n                id = {this.props.id}\n            />\n            <input\n                type=\"text\"\n                name=\"taskdescription\"\n                ref={node => (this.inputNode = node)}\n\n                onChange={this.handleChange}\n                id = {this.props.id}\n            />\n            <input\n                type=\"checkbox\"\n                name=\"iscompleted\"\n                ref={node => (this.inputNode = node)}\n         \n                onChange={this.handleChange}\n                id = {this.props.id}\n            />\n            <button class=\"btn btn-sm btn-success\" type=\"submit\">{this.props.name}</button>\n            \n        </form>\n        <button   onClick={()=>this.cancelButton(this.props.id)} class=\"btn btn-sm btn-secondary\" type=\"submit\">Cancel</button>\n        </div>\n        )\n    }\n}\n\nexport default CardForm","/mnt/1660E61660E5FC7D/AK360/REACT/React_Todolist/todolist/src/components/AddTaskPopup.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/index.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/Site.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/components/footer.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/components/header.js",["130"],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/pages/Productdetail.js",["131","132"],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/pages/Homepage.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/components/card.js",["133"],[],"/mnt/1