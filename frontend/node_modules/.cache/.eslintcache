ACT/React_Todolist/todolist/src/components/Backdrop.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/React_Todolist/todolist/src/components/Modal.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/React_Todolist/todolist/src/components/categoryform.js",["189"],[],"import React from \"react\";\nimport axios from 'axios';\n\nclass CategoryForm extends React.Component { \n    state = {\n        name: ''\n    }\n\n    handleChange = (event) => {\n        this.setState({ name: event.target.value });\n    }\n\n    handleSubmit = (event) => {\n        document.getElementById(\"categoryform\").style.display= \"none\";\n        document.getElementById(event.target.categoryname.id).style.display= \"none\";\n        document.getElementById(\"addbutton\").style.display = \"block\";\n        \n        event.preventDefault();\n        const input = event.target.categoryname.value\n        const id = event.target.categoryname.id\n\n\n        if (input.indexOf(\" \")>0 || input.length>0){\n\n            if (id != \"categoryform\"){\n                axios.post('http://localhost:8000/category-update/'+id+\"/\", { name : this.state.name })\n                .then(res => {window.location.reload(false);})\n            }\n            else{\n            \n                axios.post('http://localhost:8000/category-create/', { name : this.state.name })\n                .then(res => {window.location.reload(false);})\n            }\n        }\n        else{\n            console.log(\"Not valid input\")\n        }\n    }\n\n    cancelButton=(id)=>{\n        document.getElementById(id).style.display= \"none\";\n        document.getElementById(\"addbutton\").style.display = \"block\";\n        document.getElementById(\"category\"+id).style.display = \"block\";\n    }\n\n    render() {\n        const textboxstyle ={\n            width: \"100%\",\n            \"background-color\":\"lightgrey\",\n            \"padding\" : \"3px 13px\",\n            \"box-sizing\": \"border-box\",\n            \"border\": \"3px\",\n            \"margin-bottom\":\"20px\",\n            \"border-radius\": \"4px\",\n        }\n\n        const formstyle = {\n            \"border-color\":'black',\n            \"display\":\"none\",\n            \"padding\":\"15px\",\n            \"margin\" : \"10px\",\n        }\n\n        const buttonstyle ={\n            \"margin-left\" : \"13px\",\n            \"margin-top\" : \"5px\"\n        }\n\n        return (\n        <div class=\"card bg-dark\" style={formstyle} id={this.props.id}>\n        <h5>{this.props.head}</h5>\n        <br/>\n        <form onSubmit={this.handleSubmit}>\n            <input\n                type=\"text\"\n                name=\"categoryname\"\n                ref={node => (this.inputNode = node)}\n                style = {textboxstyle}\n                onChange={this.handleChange}\n                defaultValue={this.props.defaultvalue}\n                id = {this.props.id}\n            />\n            <section>\n            <button style={buttonstyle} class=\"btn btn-sm btn-success\" type=\"submit\">{this.props.name}</button>\n            <button  style={buttonstyle} onClick={()=>this.cancelButton(this.props.id)} class=\"btn btn-sm btn-secondary\" type=\"submit\">Cancel</button>\n            </section>\n            \n        </form>\n        </div>\n        )\n    }\n}\n\nexport default CategoryForm","/mnt/1660E61660E5FC7D/AK360/REACT/React_Todolist/todolist/src/components/card.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/React_Todolist/todolist/src/components/category.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/React_Todolist/todolist/src/components/cardform.js",["190"],[],"import React from \"react\";\nimport axios from 'axios';\n\nclass CardForm extends React.Component { \n    state = {\n        name: ''\n    }\n\n    handleChange = (event) => {\n        this.setState({ name: event.target.value });\n    }\n\n\n    render() {\n    \n        return (\n        <div class=\"card bg-dark\" id={this.props.id}>\n        <h5>{this.props.head}</h5>\n        <br/>\n        <form onSubmit={this.handleSubmit}>\n            <input\n                type=\"text\"\n                name=\"taskname\"\n                ref={node => (this.inputNode = node)}\n\n                onChange={this.handleChange}\n                id = {this.props.id}\n            />\n            <input\n                type=\"text\"\n                name=\"taskdescription\"\n                ref={node => (this.inputNode = node)}\n\n                onChange={this.handleChange}\n                id = {this.props.id}\n            />\n            <input\n                type=\"checkbox\"\n                name=\"iscompleted\"\n                ref={node => (this.inputNode = node)}\n         \n                onChange={this.handleChange}\n                id = {this.props.id}\n            />\n            <button class=\"btn btn-sm btn-success\" type=\"submit\">{this.props.name}</button>\n            \n        </form>\n        <button   onClick={()=>this.cancelButton(this.props.id)} class=\"btn btn-sm btn-secondary\" type=\"submit\">Cancel</button>\n        </div>\n        )\n    }\n}\n\nexport default CardForm","/mnt/1660E61660E5FC7D/AK360/REACT/React_Todolist/todolist/src/components/AddTaskPopup.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/index.js",[],[],["191","192"],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/Site.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/components/footer.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/components/header.js",["193"],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/pages/Productdetail.js",["194","195"],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/pages/Homepage.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/components/card.js",["196"],[],"import React from \"react\";\nimport {Link} from \"react-router-dom\";\n\nexport default function Card(props){\n\n    const imagestyle ={\n        \"object-fit\":\"contain\",\n        maxHeight:\"200px\", \n        overflow:\"hidden\", \n        maxWidth:\"300px\",\n        width:\"auto\", display: \"block\", \n        marginLeft: \"auto\", \n        marginRight: \"auto\", \n        marginTop:\"10px\"\n    }\n\n    return(\n        <div className=\"col-md-4\">\n            <div className=\"card mb-5 box-shadow\">\n                <div className=\"select2-container--classic\">\n                    <img className=\"card-img-top\" src={props.imageurl} style={imagestyle} />\n                </div>\n                <div className=\"card-body\">\n                    <p className=\"card-text\">{props.name}</p>\n                    <h4 style={{color:\"green\"}}>{props.price+ \" INR\"}</h4>\n                    <br/>\n                    <div className=\"d-flex justify-content-between align-items-center\">\n                        <div className=\"btn-group\">\n                            <Link to={props.view}><button  type=\"button\" className=\"btn btn-sm btn-outline-secondary\">View</button></Link>\n                            <Link to=\"/cart\"><button onClick={props.addtocart} type=\"button\" className=\"btn btn-sm btn-outline-info updatecart\">Add to cart</button></Link>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    );\n\n}","/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/pages/Loginpage.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/context/AuthContext.js",["197","198"],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/utils/PrivateRoute.js",["199"],[],"import {\n    Route, \n    Navigate,\n} from 'react-router-dom';\nimport { useContext } from 'react';\nimport AuthContext from '../context/AuthContext';\n\nexport default function PrivateRoute({ children }) {\n    let {user} = useContext(AuthContext)\n    return user ? children : (\n        <Navigate to=\"/login\" replace/>\n    );\n}\n  ","/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/context/ProductContext.js",["200"],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/pages/CartPage.js",["201","202","203"],[],"import React from \"react\";\nimport axios from \"axios\";\nimport CartListCard from \"../components/cartCard\";\nimport { useContext } from \"react\";\nimport ProductContext from \"../context/ProductContext\";\nimport CartContext from \"../context/CartContext\";\nimport { Navigate } from 'react-router';\nimport {increment, decrement} from '../utils/functions'\nimport Warningcard from \"../components/WarningCard\";\n\n\nexport default function CartPage(){\n    let {productList} = useContext(ProductContext)\n    let {cartList}  = useContext(CartContext)\n    let {fetchCart} = useContext(CartContext)\n    let totalprice = 0\n    let cards = []\n    fetchCart()\n    if(cartList.length>0){\n        for (let i=0  ; i<cartList.length;i++){\n            for (let j=0  ; j<productList.length;j++){\n                if(productList[j].id===cartList[i].product){\n                    let tp = parseFloat(cartList[i].count)*parseFloat(productList[j].price)\n                    totalprice+=tp\n                    cards.push(\n                        <CartListCard\n                        to={'/product/'+productList[j].id+\"/\"}\n                        name ={productList[j].name}\n                        image={\"http://127.0.0.1:8000\"+productList[j].image}\n                        price = {parseFloat(productList[j].price)}\n                        quantity = {cartList[i].count}\n                        increment = {function(){\n                            increment(productList[j].id)\n                        }}\n                        decrement = {function(){\n                            decrement(productList[j].id)\n                        }}\n                        totalprice = {tp}\n                        />\n                    )\n                }\n            }\n        }\n    }\n    else {\n        cards.push(\n            <Warningcard message=\"🛒 Cart is Empty\"/>\n        )\n    }\n    return(\n        <main role=\"main\">\n            <div className=\"album py-5 bg-light\">\n            <div className=\"container\">\n                    <div className=\"row\">\n                        {cards}\n                    </div>\n                    <div className=\"list-group-item d-flex justify-content-between\">\n                        <h4>Total price : </h4>\n                        <h3 style={{color:\"green\"}}><strong>{totalprice} INR</strong></h3>\n                    </div>\n                    <div className=\"list-group-item d-flex justify-content-between\">\n                        <a  href=\"\"><button type=\"button\" className=\"btn btn-lg btn-outline-success updatecart\">CHECKOUT</button></a>\n                    </div>\n                </div>\n            </div>\n        </main>\n    )\n}\n\n\n","/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/components/cartCard.js",["204"],[],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\n\nexport default function CartListCard(props){\n    const imagestyle ={\n        \"object-fit\":\"contain\",\n        maxHeight:\"200px\", \n        overflow:\"hidden\", \n        maxWidth:\"300px\",\n        width:\"auto\", display: \"block\", \n        marginLeft: \"auto\", \n        marginRight: \"auto\", \n        marginTop:\"20px\"\n    }\n    return(\n        <div className=\"col-md-4\">\n            <div className=\"card mb-4 box-shadow\">\n                <div className=\"select2-container--classic\">\n                    <img className=\"card-img-top\" src={props.image} style={imagestyle} />\n                </div>\n                <div className=\"card-body\">\n                    <h4 className=\"card-text\">{props.name}</h4>\n                    <h5 style={{color:\"green\"}}>{props.price+\" INR\"}</h5>\n                    <div className = \"count\">\n                        {\"quantity  : \"+props.quantity} \n                        <div className=\"btn-group\"  style={{marginLeft:\"5px\"}}>\n                            <button onClick={props.increment} type=\"button\" className=\"btn-small\">⬆️</button>\n                            <button onClick={props.decrement} type=\"button\" className=\"btn-small\">⬇️</button>\n                        </div>\n                    </div>\n                    <div style={{color:\"grey\"}}>{\"Total : \"+props.totalprice+\" INR\"}</div>\n                    <br/>\n                    <div className=\"d-flex justify-content-between align-items-center\">\n                        <br/>\n                        <div className=\"btn-group\">\n                        <Link to={props.to}><button type=\"button\" className=\"btn btn-sm btn-outline-secondary\">View</button></Link>\n                        </div>\n                    </div>\n                </div>\n            </div>\n    </div>\n    );\n}","/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/context/CartContext.js",["205","206"],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/utils/functions.js",["207"],[],"import React from \"react\"\nimport axios from \"axios\"\n\nexport let rooturl =\"http://127.0.0.1:8000\"\n\nexport function increment(id){\n    let Token = JSON.parse(localStorage.getItem('authToken')).access\n    axios.get(rooturl+'/api/cart-add/'+id+\"/\",{\n        headers:{\n            'Authorization':`Bearer ${Token}`\n        }\n    })\n\n}\n\nexport function decrement(id){\n    let Token = JSON.parse(localStorage.getItem('authToken')).access\n    axios.get(rooturl+'/api/cart-remove/'+id+\"/\",{\n        headers:{\n            'Authorization':`Bearer ${Token}`\n        }\n    })\n\n}\n\n","/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/components/WarningCard.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/pages/ProfilePage.js",["208"],[],"/mnt/1660E61660E5FC7D/AK360/REACT/Ecom/frontend/src/pages/SignupPage.js",[],[],"/mnt/1660E61660E5FC7D/AK360/REACT/E